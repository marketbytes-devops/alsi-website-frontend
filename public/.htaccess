<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Redirect non-www to www
  RewriteCond %{HTTP_HOST} ^sakivisa.com [NC]
  RewriteRule ^(.*)$ http://www.sakivisa.com/$1 [L,R=301,NC]

  # Existing rules
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>
