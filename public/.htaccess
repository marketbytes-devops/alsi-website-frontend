<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Redirect non-www to www
  RewriteCond %{HTTP_HOST} ^app\.alsi-group\.com$ [NC]
  RewriteRule ^(.*)$ https://www.app.alsi-group.com/$1 [L,R=301]

  # Existing rules
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>
